{
  "version": "1.0.0",
  "name": "atomic.bulldog",
  "private": true,
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-bundle-assets": "^2.29.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-purge-sourcemaps": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-strip-comments": "^2.5.1",
    "gulp-util": "^3.0.8",
    "kss": "^3.0.0-beta.18",
    "michelangelo": "^0.6.1",
    "onchange": "^3.3.0",
    "browser-sync": "^2.18.13"
  },
  "scripts": {
    "start":
      "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Mozilla Firefox\\firefox.exe\"",
    "kssWatch": "onchange \"dist/scss/**/*.scss\" -- npm run kss",
    "kss": "kss --config dist/scss/kss-config.json",
    "gulp": "gulp",
    "watchScss": "start npm run kssWatch && start npm run gulp watch-scss"
  }
